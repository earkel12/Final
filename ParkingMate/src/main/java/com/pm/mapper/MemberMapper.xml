<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.pm.mapper.MemberMapper">
    	<select id = "loginCheck" parameterType="String" resultType="com.pm.member.model.MemberDTO">
  		select pwd from user where id = #{id}
  	</select>
  	<select id = "userInfo" parameterType="String" resultType="String">
  		select name form user where id = #{id}
  	</select>
  </mapper>